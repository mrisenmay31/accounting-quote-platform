# Supabase Configuration (Required for multi-tenant functionality)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Airtable Configuration (Optional - fallback for development/testing)
# In production, these are loaded from the Supabase tenants table per tenant
VITE_AIRTABLE_PRICING_BASE_ID=your_pricing_base_id_here
VITE_AIRTABLE_PRICING_API_KEY=your_pricing_api_key_here

# Airtable Services Configuration (Optional - fallback for development/testing)
VITE_AIRTABLE_SERVICES_BASE_ID=your_services_base_id_here
VITE_AIRTABLE_SERVICES_API_KEY=your_services_api_key_here

# Zapier Webhook Integration (Optional - fallback for development/testing)
# In production, this is loaded from the Supabase tenants table per tenant
VITE_ZAPIER_WEBHOOK_URL=your_zapier_webhook_url_here
